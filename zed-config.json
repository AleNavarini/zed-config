[
  {
    "bindings": {
      "super-t": "terminal_panel::ToggleFocus",
      "super-y": "project_panel::ToggleFocus",
      "super-w": "pane::CloseActiveItem",
      "super-b": "workspace::ToggleLeftDock",
      "super-n": "workspace::ToggleRightDock"
    }
  },
  {
    "context": "Editor",
    "bindings": {
      "win-shift-down": "editor::DuplicateLineDown",
      "win-shift-up": "editor::DuplicateLineUp",
      "win-down": "editor::MoveLineDown",
      "win-up": "editor::MoveLineUp",
      "super-shift-down": "editor::AddSelectionBelow",
      "super-shift-up": "editor::AddSelectionAbove"
    }
  },
  {
    "context": "Editor && vim_mode == normal",
    "bindings": {
      "super-a": ["workspace::SendKeystrokes", "g g V G"]
    }
  }
]
